  <div class="span4">
    <div class="span3">
      <div class="bs-docs">
        <h4 class="text-info" >You are not following:</h4>
        <div id="templist">
        <ul class="unstyled">
          <% @not_following.each do |user| %>
            <a onClick="window.location.reload( true );" href="/follow/<%= user %>"><li><%= user %></li></a>
          <% end %>
        </ul>
        </div>
      </div>
    </div>
    <div class="span3">
      <div class="bs-docs">
        <h3 class="text-info" >Following:</h3>
        <ul>
        <% @following.each do |user| %>
          <a href="#"><li><%= user %></li></a>
        <% end %>
        </ul>
      </div>
    </div>
    <div class="span3">
      <div class="bs-docs">
        <h3 class="text-info">Followers:</h3>
        <ul>
        <% @followed.each do |user| %>
